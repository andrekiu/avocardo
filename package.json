{
  "scripts": {
    "bundle": "sh ./scripts/bundle_client.sh",
    "client-prod": "yarn run --experimental-scope-hoisting parcel build src/chrome_extension/UI.res -d build/chrome_extension",
    "client-dev": "yarn run --experimental-scope-hoisting parcel watch src/chrome_extension/UI.res -d build/chrome_extension --no-hmr",
    "clean": "yarn run rescript clean",
    "build": "yarn run rescript",
    "server-prod": "yarn run nodemon src/server/Server.bs.js",
    "server-dev": "yarn run nodemon --watch ./src/server src/server/Server.bs.js",
    "server-debug": "node --inspect src/server/Server.bs.js",
    "react-dev": "yarn run react-devtools",
    "prod": "yarn run bundle prod; yarn run client-prod"
  },
  "devDependencies": {
    "bs-platform": "^8.2.0",
    "bsb-js": "^1.1.7",
    "nodemon": "^2.0.6",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.1.1",
    "react-devtools": "^4.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "@adnelson/requery": "^0.51.0-2",
    "@glennsl/bs-json": "^5.0.2",
    "@rescript/react": "^0.10.3",
    "bs-css-dom": "^2.0.0",
    "bs-express": "^1.0.2",
    "bs-fetch": "^0.6.2",
    "bs-mysql2": "https://github.com/andrekiu/bs-mysql2.git",
    "bs-webapi": "^0.19.1",
    "dotenv": "^8.2.0",
    "mysql": "^2.18.1",
    "react": "^0.0.0-experimental-4ead6b530",
    "react-dom": "^0.0.0-experimental-4ead6b530",
    "rescript": "^9.1.2"
  }
}
