{
  "name": "Avocardo",
  "version": "0.1",
  "description": "Avocado cards to test your spanish!",
  "permissions": ["declarativeContent", "http://127.0.0.1:3000/*", "http://127.0.0.1:8097/*"],
  "content_security_policy": "script-src-elem 'self' http://127.0.0.1:8097/",
  "web_accessible_resources": ["UI.js.map"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html"
  },
  "manifest_version": 2
}